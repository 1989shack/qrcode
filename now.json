{
  "name": "qrsnapper.com",
  "version": 2,
  "alias": [
    "qrsnapper.com"
  ],
  "github": {
    "enabled": false
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src" : ".mjs$",
      "headers" : {
        "content-type": "text/javascript",
        "cache-control": "max-age=7200"
      }
    },
    {
      "src" : ".(jpg|jpeg|gif|png|svg)$",
      "headers" :{
        "cache-control": "max-age=7200"
      }
    },
    {
      "src" : ".css$",
      "headers" : {
        "cache-control": "max-age=7200"
      }
    },
    {
      "src": ".(js|json)$",
      "headers" : {
        "cache-control": "max-age=7200"
      }
    }
  ]
}
